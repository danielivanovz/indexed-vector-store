{
    "name": "indexed-vector-store",
    "version": "0.0.1",
    "license": "MIT",
    "keywords": [
        "indexeddb",
        "vector-store",
        "vector-database",
        "embeddings"
    ],
    "homepage": "https://github.com/danielivanovz/indexed-vector-db",
    "private": true,
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts",
        "release": "pnpm run build && changeset publish",
        "docs": "typedoc --out docs --plugin typedoc-plugin-markdown --theme markdown --readme none --excludeExternals --excludePrivate --excludeProtected --excludeInternal --exclude \"**/*.wasm\" src",
        "lint": "tsc"
    },
    "devDependencies": {
        "@changesets/cli": "^2.26.0",
        "@types/node": "^20.5.6",
        "tsup": "^6.5.0",
        "typedoc": "^0.25.0",
        "typedoc-plugin-markdown": "^3.16.0",
        "typescript": "^5.0.2"
    },
    "dependencies": {
        "idb": "^7.1.1"
    }
}
