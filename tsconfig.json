{
    "compilerOptions": {
        "target": "ES2020",
        "module": "commonjs",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "strictNullChecks": false,
        "skipLibCheck": true,
        "noUncheckedIndexedAccess": true,
        "noEmit": true
    }
}
